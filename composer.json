{
    "name": "zenosyne/ajax-select",
    "description": "A Laravel Nova Ajax Select field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Zenosyne\\AjaxSelect\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zenosyne\\AjaxSelect\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
